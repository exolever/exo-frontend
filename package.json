{
  "name": "openexo",
  "version": "1.0.0",
  "license": "MIT",
  "angular-cli": {},
  "private": true,
  "scripts": {
    "ng": "ng --max_old_space_size=5048",
    "postinstall": "./src/scripts/post-install-moment.sh",
    "start": "ng serve",
    "start:cli": "node ./.scripts/custom-elements-cli.js",
    "start:cli-proxy": "node ./.scripts/cli-proxy.js",
    "start:element": "ng serve --proxy-config ./.scripts/custom-element-proxy.json",
    "start:prox": "ng serve --proxy-config proxy.conf.json",
    "start:prox:highMemo": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:highMemo": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --prod --output-hashing all --source-map=false",
    "build:highMemoMaster": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --prod --output-hashing all",
    "build:highMemoNamedChunks": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --output-hashing all  --named-chunks=false",
    "doc:build": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -n 'EXO Front documentation'",
    "test": "ng test",
    "test:headless": "ng test --browsers=ChromeHeadless --watch=false --code-coverage --source-map=false",
    "test:sourcemap": "ng test --code-coverage --source-map=false",
    "e2e": "ng e2e",
    "doc:buildserve": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -n 'EXO Front documentation' -s",
    "doc:serve": "./node_modules/.bin/compodoc -s",
    "docker:pipeline": "npm run lint && npm run test:headless && ng build --aot --prod --output-hashing all",
    "lint": "ng lint openexo --format=stylish",
    "sort:en": "./node_modules/.bin/sort-json src/assets/i18n/en.json",
    "build-custom-elements": "cd ./custom-elements && node ./build-custom-elements.js"
  },
